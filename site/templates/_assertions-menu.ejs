<nav id="assertion-menu" class="sidebar js-remember-scroll-position">
    <ul>
        <%
        var order = { any: 0, boolean: 1, number: 2, string: 3, array: 4, function: 5, object: 6, Error: 7 };
        Object.keys(assertionsByType).sort(function (a, b) {
            if (typeof order[a] === 'number' && (typeof order[b] === 'undefined' || order[a] < order[b])) {
                return -1;
            } else if (typeof order[b] === 'number' && (typeof order[a] === 'undefined' || order[a] > order[b])){
                return 1;
            } else {
                return 0;
            }
        }).forEach(function (type) { %>
        <li>
            <h3><%= type %></h3>
            <ul>
                <% assertionsByType[type].forEach(function (assertion) { %>
                <li class="<%= '/' + path + '/' === assertion.url ? 'active' : '' %>">
                    <a href="<%= assertion.url %>"><%= assertion.title %></a>
                </li>
                <% }) %>
            </ul>
        </li>
        <% }) %>
    </ul>
</nav>
