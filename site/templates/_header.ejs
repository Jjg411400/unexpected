<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/static/normalize.css" type="text/css" media="screen" />
    <link rel="stylesheet" href="/static/main.css" type="text/css" media="screen" />
    <title><%= typeof title !== 'undefined' ? windowTitle + ' | Unexpected.js' : 'Unexpected.js' %></title>
</head>
<body>
    <header>
        <nav>
            <ul>
                <li class="<%- path === '' ? 'active' : '' %>"><a href="/">Unexpected</a></li>
                <li class="<%- path.match(/^assertions\/?/) ? 'active' : '' %>"><a href="<%= assertionsByType.any[0].href%>">Assertions</a></li>
            </ul>
        </nav>
    </header>
    <section id="assertions">
        <nav id="assertion-menu" class="js-remember-scroll-position">
            <ul>
                <% Object.keys(assertionsByType).forEach(function (type) { %>
                <li>
                    <%= type %>
                    <ul>
                        <% assertionsByType[type].forEach(function (assertion) { %>
                        <li class="<%= isActiveAssertion(path, type, assertion.title) ? 'active' : '' %>">
                            <a href="<%= assertion.href %>"><%= assertion.title %></a>
                        </li>
                        <% }) %>
                    </ul>
                </li>
                <% }) %>
            </ul>
        </nav>
        <div class="content">
