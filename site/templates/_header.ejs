<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/static/normalize.css" type="text/css" media="screen" />
    <link rel="stylesheet" href="/static/main.css" type="text/css" media="screen" />
    <title><%= typeof title !== 'undefined' ? title + ' | Unexpected.js' : 'Unexpected.js' %></title>
</head>
<body>
    <header>
        <nav>
            <ul>
                <li class="<%- path === '' ? 'active' : '' %>"><a href="/">Unexpected</a></li>
                <li class="<%- path.match(/^assertions\/?/) ? 'active' : '' %>"><a href="/assertions">Assertions</a></li>
            </ul>
        </nav>
    </header>
    <section id="assertions">
        <nav id="assertion-menu" class="js-remember-scroll-position">
            <ul>
                <% Object.keys(assertions).forEach(function (type) { %>
                <li>
                    <a href="/assertions/<%= type %>/"><%= type %></a>
                    <ul>
                        <% assertions[type].forEach(function (assertion) { %>
                        <li class="<%= isActiveAssertion(path, type, assertion) ? 'active' : '' %>">
                            <a href="/assertions/<%=type%>/<%=titleToId(assertion)%>"><%= assertion %></a>
                        </li>
                        <% }) %>
                    </ul>
                </li>
                <% }) %>
            </ul>
        </nav>
        <div class="content">
